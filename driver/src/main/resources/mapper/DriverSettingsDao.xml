<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.driver.db.dao.DriverSettingsDao">
    <insert id="insertDriverSettings" parameterType="com.driver.db.pojo.DriverEntity">
        INSERT INTO tb_driver_settings
        SET driver_id = #{driverId},
            settings = #{settings}
    </insert>
</mapper>